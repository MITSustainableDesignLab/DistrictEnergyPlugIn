<UserControl x:Class="DistrictEnergy.Views.ResultViews.Demands"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DistrictEnergy.Views.ResultViews"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <StackPanel x:Name="stackCustomCW">
                <Button Name="btnAddAdditionalLoad"
                        Margin="0,20,0,0" Click="btnAddAdditionalLoad_Click">
                    Add Custom Cooling Supply Module
                </Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
